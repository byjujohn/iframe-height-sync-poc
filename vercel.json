{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "iframe-content/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "vendor-component/dist/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/iframe-content/(.*)",
            "dest": "/iframe-content/$1"
        },
        {
            "src": "/vendor-component/dist/(.*)",
            "dest": "/vendor-component/dist/$1"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}